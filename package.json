{
  "name": "chat-room-server",
  "version": "1.0.0",
  "description": "Real-time chat application with Angular frontend and WebSocket backend",
  "main": "server.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"node server/server.js\" \"ng serve\"",
    "start-prod": "concurrently \"node server/server.js\" \"ng serve --host 0.0.0.0 --port 4242\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
